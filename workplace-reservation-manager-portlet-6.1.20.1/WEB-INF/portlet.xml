<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>workplace-reservation-manager</portlet-name>
		<display-name>Workplace Reservation Manager</display-name>
		<portlet-class>com.cignexdatamatics.wrm.portlet.controller.WRMController</portlet-class>
			<init-param>
			<name>home-jsp</name>
			<value>/html/wrm_jsps/admin/home.jsp</value>
		</init-param>
		<init-param>
			<name>city-view-jsp</name>
			<value>/html/wrm_jsps/admin/city_master_view.jsp</value>
		</init-param>
		<init-param>
			<name>city-edit-jsp</name>
			<value>/html/wrm_jsps/admin/city_master_edit.jsp</value>
		</init-param>
		<init-param>
			<name>facility-view-jsp</name>
			<value>/html/wrm_jsps/admin/facility_master_view.jsp</value>
		</init-param>
		<init-param>
			<name>facility-edit-jsp</name>
			<value>/html/wrm_jsps/admin/facility_master_edit.jsp</value>
		</init-param>
		<init-param>
			<name>floor-view-jsp</name>
			<value>/html/wrm_jsps/admin/floor_master_view.jsp</value>
		</init-param>
		<init-param>
			<name>floor-edit-jsp</name>
			<value>/html/wrm_jsps/admin/floor_master_edit.jsp</value>
		</init-param>
		<init-param>
			<name>location-view-jsp</name>
			<value>/html/wrm_jsps/admin/location_master_view.jsp</value>
		</init-param>
		<init-param>
			<name>location-edit-jsp</name>
			<value>/html/wrm_jsps/admin/location_master_edit.jsp</value>
		</init-param>
		<init-param>
			<name>room-view-jsp</name>
			<value>/html/wrm_jsps/admin/meeting_room_master_view.jsp</value>
		</init-param>
		<init-param>
			<name>room-edit-jsp</name>
			<value>/html/wrm_jsps/admin/meeting_room_master_edit.jsp</value>
		</init-param>
		<init-param>
			<name>booking_browse-jsp</name>
			<value>/html/wrm_jsps/common/booking_browse.jsp</value>
		</init-param>
		<init-param>
			<name>my_booking-jsp</name>
			<value>/html/wrm_jsps/common/my_booking.jsp</value>
		</init-param>
		<init-param>
			<name>room_booking_bestbrowse-jsp</name>
			<value>/html/wrm_jsps/common/room_booking_bestbrowse.jsp</value>
		</init-param>
		<init-param>
			<name>room_booking_meeting_detail-jsp</name>
			<value>/html/wrm_jsps/common/room_booking_meeting_detail.jsp</value>
		</init-param>
		<init-param>
			<name>room_booking-jsp</name>
			<value>/html/wrm_jsps/common/room_booking.jsp</value>
		</init-param>
		<init-param>
			<name>popup-jsp</name>
			<value>/html/wrm_jsps/common/popupattendees.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
	<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
			<portlet-mode>help</portlet-mode>
		</supports>
<!-- Resource Bundle -->
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Workplace Reservation Manager</title>
			<short-title>Workplace Reservation Manager</short-title>
			<keywords>Workplace Reservation Manager</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>